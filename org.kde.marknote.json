{
    "id": "org.kde.marknote",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.10",
    "sdk": "org.kde.Sdk",
    "command": "marknote",
    "finish-args": [
        "--device=dri",
        "--filesystem=xdg-documents",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland"
    ],
    "modules": [
        {
            "name": "kmime",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_BUILD_TYPE=Release"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/release-service/25.12.0/src/kmime-25.12.0.tar.xz",
                    "sha256": "c21f17b1fca6826487cd8e22cd551dee9340a52237c59670cc8c43265b0f4635",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 8763,
                        "stable-only": true,
                        "url-template": "https://download.kde.org/stable/release-service/$version/src/kmime-$version.tar.xz"
                    }
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "marknote",
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/marknote/marknote-1.3.0.tar.xz",
                    "sha256": "ff99598415a6cca5900cb4d10d2b72a6f392eafe07874b6e2eb42e9f7ab3bd28",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 371890,
                        "stable-only": true,
                        "url-template": "https://download.kde.org/stable/marknote/marknote-$version.tar.xz"
                    }
                }
            ]
        }
    ]
}
